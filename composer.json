{
    "name": "phpforce/query-builder",
    "description": "Query builder for salesforce SOQL queries",
    "type": "library",
    "keywords": [
        "salesforce",
        "apex",
        "soql"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Johannes Heinen",
            "email": "johannes.heinen@gmail.com",
            "role": "lead"
        }
    ],
    "require" : {
        "php" : ">=5.3.6",
        "ext-soap": "*",
        "ext-openssl": "*",

        "symfony/event-dispatcher" : "2.3.*",
        "doctrine/common": "2.4.*",
        "monolog/monolog": ">=1.6.0,<2.0.0",
        "jms/metadata" : "1.4.*",
        "phpforce/soap-client": "dev-master"
    },
    "require-dev" : {
        "ext-xdebug" : "*",
        "phpunit/phpunit" : "3.7.*",
        "phpunit/php-invoker": ">=1.1.0,<1.2.0"
    },
    "suggest" : {
        "ext-memcache" : "*",
        "ext-apc" : "*"
    },
    "autoload": {
        "psr-0": {
            "Joshiausdemwald\\Test\\Phpforce": "test/src/",
            "Joshiausdemwald\\Phpforce": "src/"
        }
    },
    "minimum-stability": "stable"
}